{
  "annotations": {
    "list": []
  },
  "description": "Per-provider health metrics: success rates, latency percentiles, request volume, and circuit breaker activity.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Provider Success Rates",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 90 },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "title": "Success Rate by Provider",
      "targets": [
        {
          "expr": "acteon_provider_success_rate{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 101,
      "title": "Request Volume",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "showPoints": "never",
            "stacking": { "mode": "none" }
          },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "id": 2,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Request Rate by Provider",
      "targets": [
        {
          "expr": "rate(acteon_provider_requests_total{provider=~\"$provider\"}[5m])",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "showPoints": "never",
            "stacking": { "mode": "none" }
          },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "id": 3,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Failure Rate by Provider",
      "targets": [
        {
          "expr": "rate(acteon_provider_failures_total{provider=~\"$provider\"}[5m])",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "short", "thresholds": { "steps": [{ "color": "green", "value": null }] } } },
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 16 },
      "id": 4,
      "options": { "colorMode": "background", "graphMode": "none", "textMode": "value_and_name", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Total Requests by Provider",
      "targets": [
        {
          "expr": "acteon_provider_requests_total{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 20 },
      "id": 102,
      "title": "Latency",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "ms"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 21 },
      "id": 5,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Average Latency by Provider",
      "targets": [
        {
          "expr": "acteon_provider_avg_latency_ms{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "ms"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 21 },
      "id": 6,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "p99 Latency by Provider",
      "targets": [
        {
          "expr": "acteon_provider_p99_latency_ms{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Provider" },
            "properties": [{ "id": "custom.width", "value": 150 }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*[Ll]atency.*" },
            "properties": [
              { "id": "unit", "value": "ms" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Success Rate" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 1 },
              { "id": "thresholds", "value": { "steps": [{ "color": "red", "value": null }, { "color": "orange", "value": 90 }, { "color": "green", "value": 99 }] } },
              { "id": "custom.displayMode", "value": "color-background" }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 29 },
      "id": 7,
      "options": { "showHeader": true, "sortBy": [{ "displayName": "Provider", "desc": false }] },
      "title": "Provider Health Summary",
      "targets": [
        {
          "expr": "acteon_provider_p50_latency_ms{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "p50",
          "format": "table",
          "instant": true
        },
        {
          "expr": "acteon_provider_p95_latency_ms{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "p95",
          "format": "table",
          "instant": true
        },
        {
          "expr": "acteon_provider_p99_latency_ms{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "p99",
          "format": "table",
          "instant": true
        },
        {
          "expr": "acteon_provider_avg_latency_ms{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "avg",
          "format": "table",
          "instant": true
        },
        {
          "expr": "acteon_provider_success_rate{provider=~\"$provider\"}",
          "legendFormat": "{{provider}}",
          "refId": "sr",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true },
            "renameByName": {
              "provider": "Provider",
              "Value #p50": "p50 Latency",
              "Value #p95": "p95 Latency",
              "Value #p99": "p99 Latency",
              "Value #avg": "Avg Latency",
              "Value #sr": "Success Rate"
            }
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["acteon", "provider-health"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "prometheus" },
        "hide": 0,
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "DS_PROMETHEUS",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      },
      {
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "definition": "label_values(acteon_provider_requests_total, provider)",
        "hide": 0,
        "includeAll": true,
        "label": "Provider",
        "multi": true,
        "name": "provider",
        "options": [],
        "query": "label_values(acteon_provider_requests_total, provider)",
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Acteon Provider Health",
  "uid": "acteon-provider-health",
  "version": 1
}
